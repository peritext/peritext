{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    "add-module-exports",
    "transform-class-properties",
    "transform-decorators-legacy",
    [
      "react-intl",
      {
        "messagesDir": "./translations/extractedMessages/",
        "enforceDescriptions": true
      }
    ]
  ]
}
